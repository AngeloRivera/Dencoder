<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>StartOnMount</key>
	<true/>
	<key>Label</key>
	<string>com.dustinrue.dencoder</string>
	<key>UserName</key>
	<string>root</string>
	<key>GroupName</key>
	<string>daemon</string>
	<key>KeepAlive</key>
	<false/>
	<key>OnDemand</key>
	<false/>
	<key>RunAtLoad</key>
	<true/>
	<key>AbandonProcessGroup</key>
	<true/>
	<key>WorkingDirectory</key>
	<string>/tmp</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/dencoder.py</string>
	</array>
	<key>StandardOutPath</key>
	<string>dencoder.log</string>
	<key>StandardErrorPath</key>
	<string>dencoder.err</string>
</dict>
</plist>
