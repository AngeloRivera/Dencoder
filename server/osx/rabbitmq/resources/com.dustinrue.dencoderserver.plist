<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.dustinrue.encoderserver</string>
	<key>UserName</key>
	<string>root</string>
	<key>GroupName</key>
	<string>daemon</string>
	<key>KeepAlive</key>
	<dict>
		<key>SuccessfulExit</key>
		<false/>
	</dict>
	<key>OnDemand</key>
	<false/>
	<key>RunAtLoad</key>
	<true/>
	<key>WorkingDirectory</key>
	<string>/tmp</string>
	<key>ProgramArguments</key>
	<array>
		<string>/opt/rabbitmq/sbin/rabbitmq-service.py</string>
	</array>
	<key>StandardOutPath</key>
	<string>rabbitmq.log</string>
	<key>StandardErrorPath</key>
	<string>rabbitmq.error</string>
</dict>
</plist>
